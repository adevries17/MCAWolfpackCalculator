<Application x:Class="MCAWolfpackCalc.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:MCAWolfpackCalc"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <x:Array x:Key="Langs" Type="sys:String">
            <sys:String>English</sys:String>
            <sys:String>German</sys:String>
            <sys:String>Russian</sys:String>
        </x:Array>

        <x:Array x:Key="Detonators" Type="sys:String">
            <sys:String>Magnetic</sys:String>
            <sys:String>Impact</sys:String>
        </x:Array>
    </Application.Resources>
</Application>
